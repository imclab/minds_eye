<h1>New photo</h1>

<% form_for(@photo) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :flickr_id %><br />
    <%= f.text_field :flickr_id %>
  </p>
  <p>
    <%= f.label :taken_at %><br />
    <%= f.datetime_select :taken_at %>
  </p>
  <p>
    <%= f.label :uploaded_at %><br />
    <%= f.datetime_select :uploaded_at %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :license %><br />
    <%= f.text_field :license %>
  </p>
  <p>
    <%= f.label :flickr_url %><br />
    <%= f.text_field :flickr_url %>
  </p>
  <p>
    <%= f.label :flickr_search_id %><br />
    <%= f.text_field :flickr_search_id %>
  </p>
  <p>
    <%= f.label :image_filename %><br />
    <%= f.text_field :image_filename %>
  </p>
  <p>
    <%= f.label :image_width %><br />
    <%= f.text_field :image_width %>
  </p>
  <p>
    <%= f.label :image_height %><br />
    <%= f.text_field :image_height %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', photos_path %>
