<h1>New flickr_search</h1>

<% form_for(@flickr_search) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :search_parameter %><br />
    <%= f.text_field :search_parameter %>
  </p>
  <p>
    <%= f.label :search_type %><br />
    <%= f.text_field :search_type %>
  </p>
  <p>
    <%= f.label :start_page %><br />
    <%= f.text_field :start_page %>
  </p>
  <p>
    <%= f.label :end_page %><br />
    <%= f.text_field :end_page %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', flickr_searches_path %>
